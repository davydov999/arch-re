{
  "env": {
    "production": {
      "presets": [["es2015", { "modules": false }], "react"],
      "plugins": [
        "transform-object-rest-spread",
        "transform-regenerator",
        ["lodash", { "id": ["lodash", "recompose"] }]
      ]
    },
    "development": {
      "presets": [["es2015", { "modules": false }], "react"],
      "plugins": [
        "react-hot-loader/babel",
        "transform-object-rest-spread",
        "transform-regenerator",
        ["lodash", { "id": ["lodash", "recompose"] }]
      ]
    }
  }
}